.bg{
  width: 100%;
  height: 150rpx;
  position: fixed;
  top: 0;
  left: 0;
}
.list{
  background: #f5f5f5;
  height: 100%;
  padding-top: 10rpx;
}
.item{
  background: #fff;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #333;
}
.item+.item{
  border-top: 1px solid #f5f5f5;
}
.item-icon{
  width: 30rpx;
  height: 30rpx;
}